@model Tastebook.Models.IndexViewModel

<h3>Hello, @User.Identity.Name</h3>

<h5>Moje komentarze</h5>
<ul>
    @foreach (var comment in @Model.MyComments)
    {
        <li>[@comment.Created] @comment.Text</li>
    }
</ul>

<hr />

<h5>Moje przepisy</h5>
<ul>
    @foreach (var recipe in @Model.MyRecipes)
    {
        <li>[@recipe.Created] @recipe.Name</li>
    }
</ul>